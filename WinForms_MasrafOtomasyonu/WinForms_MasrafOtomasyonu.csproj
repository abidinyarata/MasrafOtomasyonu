<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <Authors>Abidin YARATA</Authors>
    <Product>MasrafOtomasyonu</Product>
    <PackageId>WinForms_MasrafOtomasyonu</PackageId>
    <Description>Masraf Otomasyonu; yapılan harcamaların tutulması ve raporlaması. Yöneticiler çalışanlarının masraflarını onaylar. Onaylanan masraflar muhasebe ye gider. Muhasebe ödeme yapınca süreç tamamlanır</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ExcelMapper" Version="5.2.352" />
    <PackageReference Include="MFramework.Services.FakeData" Version="1.0.18" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>